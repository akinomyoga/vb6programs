Public Function eee(a1 As Integer, a2 As Integer, a3 As Integer, a4 As Integer, e As Double) As Double
eee = Int((Text3(a1).Text * Text3(a2).Text - Text3(a3).Text * Text3(a4).Text) / e + 0.5) / 1000
End Function
