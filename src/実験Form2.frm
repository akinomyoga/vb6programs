Private Sub Command8_Click()
Dim c As Double, d As Double, e1(8), e As Double
For a = 0 To 2
c = 1
d = -1
For b = 0 To 2
a1 = (a + b) Mod 3
a2 = (a - b) Mod 3
If a2 < 0 Then a2 = a2 + 3
c = c * Text3(a1 + 3 * b).Text
d = d * Text3(a2 + 3 * b).Text
Next b
e = e + c + d
Next a
e = e / 1000
e1(0) = eee(4, 8, 5, 7, e)
e1(1) = eee(2, 7, 1, 8, e)
e1(2) = eee(1, 5, 2, 4, e)
e1(3) = eee(5, 6, 3, 8, e)
e1(4) = eee(0, 8, 2, 6, e)
e1(5) = eee(2, 3, 0, 5, e)
e1(6) = eee(3, 7, 4, 6, e)
e1(7) = eee(1, 6, 0, 7, e)
e1(8) = eee(0, 4, 1, 3, e)
For a = 0 To 8
Text3(a).Text = e1(a)
Next a
End Sub

